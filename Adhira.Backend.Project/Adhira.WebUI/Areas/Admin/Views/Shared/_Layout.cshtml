    <script type="text/javascript" src="~/admin/files/bower_components/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="~/admin/files/bower_components/jquery-ui/jquery-ui.min.js"></script>
    <script type="text/javascript" src="~/admin/files/bower_components/popper.js/dist/umd/popper.min.js"></script>
